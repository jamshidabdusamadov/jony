{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "techbrotherstutorial"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"azurebloblink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azurebloblink'"
		},
		"azuresqllink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuresqllink'"
		},
		"linktoblobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linktoblobstorage'"
		},
		"privateendpointenabled_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'privateendpointenabled'"
		},
		"linktoazureblobstoragewithmultiplefiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linktoazureblobstoragewithmultiplefiles'"
		},
		"privateendpoint1_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/559b708b-e9de-4c26-b683-9e268fd75c9c/resourceGroups/DEMO/providers/Microsoft.Sql/servers/javoxir"
		},
		"privateendpoint1_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"privateendpoint1_properties_ipAddress": {
			"type": "object",
			"defaultValue": null
		},
		"privateendpoint1_properties_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/0ca33b6d-85cb-49d6-90d4-e28a47d9eabd/resourceGroups/vnet-0ca33b6d-eastus-67-rg/providers/Microsoft.Network/privateEndpoints/techbrotherstutorial.privateendpoint1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "techbrotherstutorial_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"azurebloblink_connectionString": {
						"value": "[parameters('azurebloblink_connectionString')]"
					},
					"azuresqllink_connectionString": {
						"value": "[parameters('azuresqllink_connectionString')]"
					},
					"linktoblobstorage_connectionString": {
						"value": "[parameters('linktoblobstorage_connectionString')]"
					},
					"linktoazureblobstoragewithmultiplefiles_connectionString": {
						"value": "[parameters('linktoazureblobstoragewithmultiplefiles_connectionString')]"
					},
					"privateendpointenabled_connectionString": {
						"value": "[parameters('privateendpointenabled_connectionString')]"
					},
					"privateendpoint1_properties_privateLinkResourceId": {
						"value": "[parameters('privateendpoint1_properties_privateLinkResourceId')]"
					},
					"privateendpoint1_properties_groupId": {
						"value": "[parameters('privateendpoint1_properties_groupId')]"
					},
					"privateendpoint1_properties_ipAddress": {
						"value": "[parameters('privateendpoint1_properties_ipAddress')]"
					},
					"privateendpoint1_properties_resourceId": {
						"value": "[parameters('privateendpoint1_properties_resourceId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "techbrotherstutorial_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'techbrotherstutorial_ArmTemplate_0')]"
			]
		}
	]
}